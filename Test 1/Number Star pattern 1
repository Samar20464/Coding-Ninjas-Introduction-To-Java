/*

 Number Star pattern 1
Send Feedback
Print the following pattern for given number of rows.
Input format :

Integer N (Total number of rows)

Output Format :

Pattern in N lines

Sample Input :

   5

Sample Output :

 5432*
 543*1
 54*21
 5*321
 *4321



*/

import java.util.Scanner;
public class runner {

	public static void main(String[] args) {
		// Write your code here
		Scanner in = new Scanner(System.in);
        int n = in.nextInt();

        int i =n;

        while (i>0)
        {
            int j =1;
            int k =n;
            while (j<=n)
            {
                if(i==j)
                    System.out.print("*");
                else
                    System.out.print(k);
                k--;
                j++;
            }
            System.out.println();
            i--;
        }

	}
}
